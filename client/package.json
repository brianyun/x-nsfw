{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "next build && next export && firebase deploy --only hosting:client",
		"export": "next export"
	},
	"dependencies": {
		"@apollo/client": "3.7.7",
		"@next/font": "13.1.0",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.10",
		"apollo-upload-client": "17.0.0",
		"axios": "1.3.2",
		"eslint": "8.30.0",
		"eslint-config-next": "13.1.0",
		"firebase": "9.15.0",
		"next": "13.1.0",
		"next-images": "1.8.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"recoil": "0.7.6",
		"styled-components": "5.3.6",
		"typescript": "4.9.4"
	},
	"devDependencies": {
		"@types/node": "18.11.18",
		"@types/styled-components": "5.1.26"
	},
	"babel": {
		"env": {
			"development": {
				"presets": [
					"next/babel"
				],
				"plugins": [
					[
						"styled-components",
						{
							"ssr": true,
							"displayName": true
						}
					]
				]
			},
			"production": {
				"presets": [
					"next/babel"
				],
				"plugins": [
					[
						"styled-components",
						{
							"ssr": true,
							"displayName": false
						}
					]
				]
			}
		}
	}
}
